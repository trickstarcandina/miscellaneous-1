function get_id(obj){return(typeof(obj.id)=='string'&&obj.id!='')?obj.id:'';}
function get_class(obj){return(typeof(obj.className)=='string'&&obj.className!='')?obj.className:'';}
function get_cookies(cName){var cookie;var aCookie=document.cookie.split('; ');for(var i=0;i<aCookie.length;i++){cookie=aCookie[i].split('=');if(cookie[0]==cName)return unescape(cookie[1]);}
return null;}
function set_cookies(cName,val){var date=new Date();date.setTime(date.getTime()+24*60*60*1000);if((cName+"="+escape(val)).length<=4000){document.cookie=cName+'='+escape(val)+"; expires="+date.toGMTString()+"; path=/";return true;}else if(confirm(_lang[lang]["Cookie_cut_auto"]))return null;}
function del_cookies(cName){var date=new Date();date.setTime(date.getTime()-24*60*60*1000);if((cName+"="+escape(val)).length<=4000){document.cookie=cName+'='+escape(val)+"; expires="+date.toGMTString()+"; path=/";return true;}else if(confirm(_lang[lang]["Cookie_cut_auto"]))return null;}
